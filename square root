//bir sayının karekökünü bulma
#include <stdio.h>
#include <math.h>
int main()
{
    int number,squareroot;
    
    printf("enter a positive number:");
    scanf("%d",&number);
    
    if(number<0){
        printf("please do not this");
    }else{
        squareroot=sqrt(number);
        if(squareroot*squareroot==number){
           
            printf("square root of:%d\n",squareroot);
        }else{
            printf("no its not\n");
        }
    }
    
  

    return 0;
}
