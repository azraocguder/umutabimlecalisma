#include <stdio.h>


int main()
{
  int number1,number2,number3;
  
  printf("enter three numbers:");
  scanf("%d %d %d",&number1,&number2,&number3);
  
  if(number1>number2 && number1>number3){
      printf("the biggest number is:%d\n",number1);
  }else if(number2>number1 && number2>number3){
      printf("the biggest number is:%d\n",number2);
  }else{
      printf("the biggest number is:%d\n",number3);
  }
  
  
  if(number1<number2 && number1<number3){
      printf("the smallest number is:%d\n",number1);
  }else if(number2<number1 && number2<number3){
      printf("the smallest number is:%d\n",number2);
  }else{
      printf("the smallest number is:%d\n",number3);
  }

    return 0;
}
