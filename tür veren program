#include <stdlib.h>
#include <stdio.h>
//klavyeden girilen değerin türünü veren program

int main() {
    char a;
    printf("a'yı giriniz:");
    scanf("%c",&a);
    
    if ((a>= 'a'&& a<= 'z') || (a>= 'A'&& a<= 'Z'))
    {
    printf("Girdiginiz deger bir harf", a);

    }
    else if (a>='0' && a<='9')
{
    printf("girdiğiniz deger bir digit");
}

else
{
    printf("girdiğiniz değer özel karakter");
}
    return 0;
}
