import UIKit

func myFunction(){
    print("my function")
}

myFunction()


// input output & return

func sumFunction(x:Int,y: Int) -> Int{
    return x + y
}
let myFunctionVariable = sumFunction(x: 10, y:20)
print(myFunctionVariable)


func logicFunction(a:Int,b:Int)->String {
    if a > b {
        return "greater"
    }else {
        return "less"
    }
}

logicFunction(a: 10, b: 0)
