// iç içe döngü (şekil)
#include <stdio.h>

int main() {
    
    int i,j,number;
    
  printf("enter a number");
  scanf("%d",&number);
  
  for(i=1;i<=number;i++){
      for(j=1;j<=i;j++){
          printf("#");
      }//sütunları
      
      printf("\n");
  }//dış for satırları 
    
    
    
    return 0;
}
